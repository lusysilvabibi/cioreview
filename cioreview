<script >

  function region_cookie(val){
    var cookieName = 'countryregion';
    var cookieValue = val;
    var myDate = new Date();
    myDate.setMonth(myDate.getMonth() + 120);
    //alert(myDate);
    if(cookieValue=='APAC' || cookieValue=='Europe'){
    
    } else{
      document.cookie = cookieName +"=" + cookieValue + ";expires=" + myDate 
                      + ";domain=.cioreview.com;path=/";
       }             
  }

</script>


