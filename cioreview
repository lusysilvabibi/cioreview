<script type="text/javascript">
$(function(){ 
   if ($('#sticky_new').length) { 
      var el = $('#sticky_new');
      var stickyTop = $('#sticky_new').offset().top;
      var stickyHeight = $('#sticky_new').height();

      $(window).scroll(function(){ 
          var limit = $('#footer').offset().top - $('#sticky_new').height() - 20;

          var windowTop = $(window).scrollTop(); 

          if( (stickyTop < windowTop) && $( window ).width() > '1005') 
          {
            if($('#desc_hig').height()>700)
                    {
                        $('#sticky_new').css({position: 'fixed',left:'67%', top: '15px', width: '265px'});
                       
                        if($( window ).width() > '830' && $( window ).width() < '951')
                        {
                            $('#sticky_new').css({width:'320px'});
                        }
                    }
                    else
                    {
                        $('#sticky_new').css({position: 'static', top: '0px'});
                        
                    }
          }
            
        else 
        {
                $('#sticky_new').css({position: 'static', top: '0px'});
                
        }

          if (limit < windowTop) {
          var diff = limit - windowTop;
          el.css({top: diff});
          }
        });
   }
});
</script>
