<script type="text/javascript">
    function set_cookie(){
      $( "#footerr" ).hide();
      var cookieName = 'cioreview';
      var cookieValue = 'cioreview';
      var myDate = new Date();
      myDate.setMonth(myDate.getMonth() + 120);
      //alert(myDate);
      document.cookie = cookieName +"=" + cookieValue + ";expires=" + myDate 
                        + ";domain=.cioreview.com;path=/";
    }
</script>

